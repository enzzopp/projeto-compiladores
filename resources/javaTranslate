 import java.util.Scanner;  public class Code {   public static void main (String[] args) {  Scanner scanner = new Scanner (System.in);  int  a  =  scanner.nextInt  (  )  ;  float  b  =  scanner.nextFloat  (  )  ;  String  b  =  scanner.nextLine  (  )  ;  int  teste  =  2  +  2  ;  float  teste2  =  2.2  +  2.4  ;  String  teste3  =  "testando"  ;  if   (  a   ==   1  )  {    b  =  2  ;  int  c  =  scanner.nextInt  (  )  ;  System.out.printf  (  b  )  ;  System.out.printf  (  2  +  2  )  ;  System.out.printf  (  2  *  2  )  ;  System.out.printf  (  2  /  2  )  ;  System.out.printf  (  (  2  /  2  )  )  ;  }      else  {  int  d  =  scanner.nextInt  (  )  ;    b  =  3  ;  System.out.printf  (  b  )  ;  }  System.out.printf  (  "oi"  )  ;  System.out.printf  (  a  )  ;  System.out.printf  (  2  +  2  )  ;  int  e  =  scanner.nextInt  (  )  ;  for  (  int  i  =  0  ;  i   <   10  ;  i  =  i  +  1  )  {    a  =  2  ;  System.out.printf  (  a  )  ;  }  while  (  a   ==   2  )  {    a  =  1  ;  }  boolean  a  =  true  ;  boolean  b  =  false  ;    scanner.close();  }   }  